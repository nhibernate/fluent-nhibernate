{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "labels": [
    "build"
  ],
  "packageRules": [
    {
      "matchSourceUrls": [
        "https://github.com/nunit/nunit"
      ],
      "groupName": "NUnit"
    },
    {
      "matchSourceUrls": [
        "https://github.com/nunit/nunit-console"
      ],
      "groupName": "NUnit.Console"
    }
  ]
}
